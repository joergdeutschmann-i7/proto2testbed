{
    "settings": {
        "machines_internet_access": true,
        "auto_dismantle_seconds": 180,
        "management_network": "172.16.99.0/24"
    },
    "networks": [
        {
            "name": "exp0",
            "physical_ports": []
        }
    ],
    "machines": [
        {
            "name": "vma",
            "diskimage": "/root/debian-test.qcow2",
            "setup_script": "vma/setup.sh",
            "environment": {
                "VARIABLE": "Hello from VM A Setup!"
            },
            "cores": 2,
            "memory": 1024,
            "networks": [
                "exp0"
            ],
            "collectors": [
                {
                    "collector": "iperf3-server",
                    "name": "vma-iperf-server",
                    "delay": 0,
                    "runtime": 50,
                    "settings": {}
                },
                {
                    "collector": "procmon",
                    "name": "procmon-vma",
                    "delay": 5,
                    "runtime": 20,
                    "settings": {
                        "interfaces": ["enp0s3"],
                        "system": true
                    }
                }
            ]
        },
        {
            "name": "vmb",
            "diskimage": "/root/debian-test.qcow2",
            "setup_script": "vmb/setup.sh",
            "environment": {
                "VARIABLE": "Hello from VM B Setup!"
            },
            "cores": 2,
            "memory": 1024,
            "networks": [
                "exp0"
            ],
            "collectors": [
                {
                    "collector": "iperf3-client",
                    "name": "vma-iperf-client",
                    "delay": 1,
                    "runtime": 30,
                    "settings": {
                        "host": "10.0.0.1"
                    }
                },
                {
                    "collector": "procmon",
                    "name": "procmon-vmb",
                    "delay": 5,
                    "runtime": 20,
                    "settings": {
                        "interfaces": ["enp0s3"],
                        "system": true
                    }
                }
            ]
        }
    ]
}