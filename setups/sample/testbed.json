{
    "settings": {
        "machines_internet_access": true,
        "auto_dismantle_seconds": 180,
        "management_network": "172.16.99.0/24"
    },
    "networks": [
        {
            "name": "exp0",
            "physical_ports": []
        }
    ],
    "machines": [
        {
            "name": "vma",
            "diskimage": "/root/debian-test.qcow2",
            "setup_script": "vma/setup.sh",
            "environment": {
                "VARIABLE": "Hello from VM A Setup!"
            },
            "cores": 2,
            "memory": 1024,
            "networks": [
                "exp0"
            ],
            "collectors": [
                {
                    "collector": "iperf3-client",
                    "name": "vmb-iperf3-client",
                    "delay": 1,
                    "timeout": 20,
                    "settings": {
                        "host": "127.0.0.1",
                        "time": 30
                    }
                }
            ]
        },
        {
            "name": "vmb",
            "diskimage": "/root/debian-test.qcow2",
            "setup_script": "vmb/setup.sh",
            "environment": {
                "VARIABLE": "Hello from VM B Setup!"
            },
            "cores": 2,
            "memory": 1024,
            "networks": [
                "exp0"
            ],
            "collectors": [
                {
                    "collector": "iperf3-client",
                    "name": "vmb-iperf3-client",
                    "delay": 1,
                    "timeout": 20,
                    "settings": {
                        "host": "127.0.0.1",
                        "time": 30
                    }
                }
            ]
        }
    ]
}